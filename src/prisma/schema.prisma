generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Post {
  id             Int      @id @default(autoincrement())
  userId         String
  content        Json
  title          String
  urlTitle       String
  SEOTitle       String
  SEODescription String
  publishedAt    DateTime
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt()
}
